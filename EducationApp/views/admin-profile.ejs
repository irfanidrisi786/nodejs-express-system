<% include partials/admin-menu.ejs %>
        <!-- Page wrapper  -->
    <div class="page-wrapper">
        <!-- Container fluid  -->
        <div class="container-fluid">
            <!-- Start Page Content -->
            <div class="row">
                <!-- Column -->
                <div class="col-8">
                    <div class="card">
                        <div class="card-body">
                            <h5 class="card-title m-b-0">Admin Profile</h5>
                        </div>
                        <table class="table">
                            <tbody>
                                <% accounts.forEach((accounts, index) => { %>
                                <tr>
                                    <th scope="row">Name</th>
                                    <td>-</td>
                                </tr>
                                <tr>
                                    <th scope="row">Surname</th>
                                    <td>-</td>
                                </tr>
                                <tr>
                                    <th scope="row">E-mail</th>
                                    <td><%= accounts.email %></td>
                                </tr>
                                <tr>
                                    <th scope="row">Gender</th>
                                    <td>-</td>
                                </tr>
                                <tr>
                                    <th scope="row">Age</th>
                                    <td>-</td>
                                </tr>
                                <tr>
                                    <th scope="row">Address</th>
                                    <td>-</td>
                                </tr>
                                <tr>
                                    <th scope="row">Status</th>
                                    <td>
                                        <% switch (accounts.user_class) {
                                        case 1 : %>
                                        Admin
                                        <% break;
                                        case 2 : %>
                                        Teacher
                                        <% break;
                                        case 3 : %>
                                        Student
                                        <% break;
                                        } %>
                                    </td>
                                </tr>
                                <tr>
                                    <th scope="row">Telephone Number</th>
                                    <td>-</td>
                                </tr>
                                <% }) %>
                            </tbody>
                        </table>
                        <div class="p-l-30 p-10"><a href="javascript:void(0)" class="btn btn-sm btn-success btn-rounded">Edit Your Profile</a></div>
                    </div>
                </div>
            </div>
        </div>

<% include partials/footer.ejs %>
